  "repository": {
    "type": "git",
    "url": "https://github.com/footearth/footearth.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/footearth/footearth.github.io/issues"
  },
  "homepage": "https://github.com/footearth/footearth.github.io",
  "Keywords": [
    "docker",
    "daocloud",
    "gitbook",
    "blog"
  ],
  "bin": {},
  "scripts": {
    "start": "pm2 dump && pm2 startOrRestart ./bin/gitbook.pm2.json",
    "stop": "pm2 kill",
    "restart": "npm stop; npm start",
    "clean": "rm -rf ./src/_book; rm -rf ./build; rm -rf ./dist",
    "build": "gitbook build ./src; mv ./src/_book/ ./build",
    "serve": "hs ./build",
    "docker_build": "docker build --rm -t mooxe/gitbook ./",
    "docker_in": "docker run -t -i -p 80:3000 mooxe/gitbook /bin/bash/",
    "docker_run": "docker run -d -p 80:3000 mooxe/gitbook",
    "test": ""
  },
  "dependencies": {},
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "gulp": "*",
    "gulp-gh-pages": "*",
    "npb-coffee": "*",
    "run-sequence": "*",
    "shelljs": "^0.5.0",
    "toa": "*",
    "toa-static": "*"
  }
}
